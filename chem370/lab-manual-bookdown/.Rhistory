#Loading the rvest package
library('rvest')
#Specifying the url for desired website to be scraped
url <- 'https://www.wcu.edu/coronavirus/reporting.aspx'
#Reading the HTML code from the website
webpage <- read_html(url)
#Using CSS selectors to scrape the webings section
web_data_html <- html_nodes(webpage,'.twocolumns')
#Converting the webing data to text
web_data <- html_text(web_data_html) %>%
str_replace_all("[ \n]", "")
newdata <- data.frame(
substr(web_data[c(1, 3)], 10, nchar(web_data)[c(1, 3)]),
substr(web_data[c(2, 4)], 9, nchar(web_data)[c(2, 4)])
)
colnames(newdata) <- c("current_employee", "cumulative_employee", "current_student", "cumulative_student",)
